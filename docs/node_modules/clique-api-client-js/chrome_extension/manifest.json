{
    "name" : "CliqueApi Screenshare",
    "author": "Sklyarov Ivan",
    "version" : "3.4",
    "manifest_version" : 2,
    "minimum_chrome_version": "34",
    "description" : "Clique api client chrome screenshare extension",
    "background": {
        "scripts": ["background-script.js"],
        "persistent": false
    },
    "externally_connectable": {
        "matches": ["https://localhost/*"]
    },
    "icons" : {
        "48" : "icon.png"
    },
    "permissions": [
        "desktopCapture"
    ],
    "web_accessible_resources": [
        "icon.png"
    ]
}